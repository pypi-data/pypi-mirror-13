{
    "distances": {
        "type": "list",
        "length": 8,
        "item_type": "number",
        "item_range": [0, 4]
    },
    "leds": {
        "type": "list",
        "length": 8,
        "item_type": "enum",
        "item_values": ["on", "off", "null"]
    },
    "battery": {
        "type": "number",
        "range": [0, 100]
    },
    "date": {
        "type": "date",
        "variance": 5
    },
    "motor": {
        "type": "list",
        "length": 2,
        "item_type": "dict",
        "item_keys": ["speed", "turn_radius"],
        "item_speed_type": "number",
        "item_speed_range": [0, 5],
        "item_turn_radius_type": "number",
        "item_turn_radius_range": [0, 3]
    },
    "acceleration": {
        "type": "dict",
        "keys": ["x", "y", "z"],
        "x_type": "number",
        "x_range": [0, 40],
        "y_type": "number",
        "y_range": [0, 30],
        "z_type": "number",
        "z_range": [0, 30]
    },
    "rotation": {
        "type": "list",
        "length": 3,
        "item_type": "number",
        "item_range": [0, 360]
    }
}
