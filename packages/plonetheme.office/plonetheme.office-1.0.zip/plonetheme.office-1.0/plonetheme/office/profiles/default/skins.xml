<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Sunburst Theme">
   <object name="plonetheme_office"
        meta_type="Filesystem Directory View"
        directory="plonetheme.office:skins/plonetheme_office"/>
    <skin-path name="*">
        <layer name="plonetheme_office"
                insert-after="custom"/>
    </skin-path>
    <skin-path name="mycustom_skin">
        <layer name="plonetheme_office"
           insert-after="custom"/>
    </skin-path>
</object>
