#  Module configuration file {{package}}
#
# While this demo can run on condor it is slower
# runtime.platforms=command,condor
runtime.platforms=command

[command]
environments=Staged,Matlab
executable  = {{package}}
files = {{package}}, {{package}}.ctf, java.opts

[condor]
# Condor needs a single executable in the local directory
# extra_requirements is part of the default submit script
# extra_requirements = (Memory > 3000)
request_memory = 7900






