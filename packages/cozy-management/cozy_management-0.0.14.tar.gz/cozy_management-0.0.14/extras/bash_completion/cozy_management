###-begin-cozy_management-completion-###
#
# cozy_management command completion script
#
# Installation: cozy_management completion >> ~/.bashrc  (or ~/.zshrc)
# Or, maybe: cozy_management completion > /usr/local/etc/bash_completion.d/cozy_management
#

_cozy_management()
{
	local cur prev opts
	COMPREPLY=()
	cur="${COMP_WORDS[COMP_CWORD]}"
	prev="${COMP_WORDS[COMP_CWORD-1]}"
	opts="ping_couchdb get_admin get_couchdb_admins delete_token create_token reset_token get_cozy_param normalize_cert_dir get_crt_common_name clean_links make_links generate_certificate regenerate_dhparam is_cozy_registered fix_oom_score"

	COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
	return 0
}

complete -F _cozy_management cozy_management
###-end-cozy_management-completion-###
