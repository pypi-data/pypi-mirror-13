{% set timeday = now.strftime('%H:%M:%S') %}
{% set fulltime = now.strftime('%Y-%m-%dT%H:%M:%S-00') %}
{% set isodate = now.strftime('%Y-%m-%d') %}
<severity>NONE</severity>
<location>not set</location>
<keywords>none</keywords>
<time>{{ timeday }}</time>
<isodate>{{ isodate }}</isodate>
<author>{{ author }}</author>
<category>USERLOG</category>
<title>{{ title }}</title>
<metainfo>{{ fulltime }}.xml</metainfo>
<link>{{ link }}</link>
<file>{{ file }}</file>
<text>{{ text }}</text>
