#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pycomon==1.1.1','console_scripts','pycomon'
__requires__ = 'pycomon==1.1.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('pycomon==1.1.1', 'console_scripts', 'pycomon')()
    )
