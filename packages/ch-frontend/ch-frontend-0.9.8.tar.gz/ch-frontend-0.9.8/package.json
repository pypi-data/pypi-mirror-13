{
  "name": "chassishub",
  "version": "0.9.8",
  "description": "ChassisHub is an online marketplace for new and pre-owned medium / heavy commercial vehicles including trucks, trailers, cranes, buses and tractors as well as their spare parts.",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint && ./node_modules/.bin/mocha --ui qunit test/index.js",
    "build": "./node_modules/.bin/gulp build",
    "clean": "./node_modules/.bin/gulp clean",
    "start": "./node_modules/.bin/gulp start",
    "watch": "./node_modules/.bin/gulp watch",
    "postinstall": "npm run build",
    "lint": "./node_modules/.bin/jscs src && ./node_modules/.bin/jscs test",
    "serve": "./node_modules/.bin/gulp serve",
    "pip-build": "npm run build && python setup.py sdist",
    "pip-upload": "npm run pip-build && python setup.py sdist upload"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/creatorrr/ch-frontend.git"
  },
  "author": "diwank.singh@gmail.com",
  "license": "None",
  "bugs": {
    "url": "https://github.com/creatorrr/ch-frontend/issues"
  },
  "homepage": "https://chassishub.com",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.21",
    "babel-plugin-rewire": "^0.1.22",
    "babel-runtime": "^5.8.20",
    "babelify": "^5.0.3",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "cloudinary": "^1.2.6",
    "connect-modrewrite": "^0.8.2",
    "envify": "^3.3.0",
    "exorcist": "^0.4.0",
    "gulp": "^3.8.7",
    "gulp-connect": "^2.2.0",
    "gulp-jsonminify": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "http-proxy-middleware": "^0.9.0",
    "jquery-deferred": "^0.3.0",
    "jscs": "^2.7.0",
    "l10ns": "^2.2.5",
    "mkdirp": "^0.5.0",
    "mocha": "^2.3.4",
    "rewire": "^2.5.1",
    "rimraf": "^2.4.4",
    "riotify": "^0.1.2",
    "sinon": "^1.17.2",
    "uglifyify": "^3.0.1",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "backbone": "^1.2.3",
    "blueimp-file-upload": "^9.11.2",
    "exoskeleton": "^0.7.0",
    "grapnel": "^0.6.3",
    "jquery": "^2.1.1",
    "jquery.cloudinary": "^1.0.25",
    "jquery.iframe-transport": "^1.0.0",
    "lodash": "^3.10.1",
    "q": "^1.2.0",
    "raven-js": "^1.3.0",
    "riot": "^2.3.11",
    "riotcontrol": "^0.0.3",
    "store": "^1.3.17",
    "waypoints": "^4.0.0"
  }
}
