<?xml version="1.0" encoding="utf-8"?>
<!-- $Id: publiquiz2html5.pfvar.xml 00122795d279 2015/06/20 21:50:27 Patrick $ -->
<publiforge version="1.0">
  <variables>
    <group>
      <label xml:lang="en">HTML 5</label>
      <label xml:lang="fr">HTML 5</label>

      <var name="onefile" type="boolean">
        <default>false</default>
        <label xml:lang="en">Result in one file</label>
        <label xml:lang="fr">Résultat en un seul fichier</label>
      </var>

      <var name="toc" type="boolean">
        <default>true</default>
        <label xml:lang="en">With a main table of content</label>
        <label xml:lang="fr">Avec un sommaire principal</label>
      </var>

      <var name="toc_division_depth" type="select">
        <default>1</default>
        <option>0</option><option>1</option><option>2</option>
        <option>3</option><option>4</option><option>5</option>
        <label xml:lang="en">Depth of divisions in main table of content</label>
        <label xml:lang="fr">Profondeur des divisions dans le sommaire principal</label>
      </var>

      <var name="toc_section_depth" type="select">
        <default>1</default>
        <option>0</option><option>1</option><option>2</option>
        <option>3</option><option>4</option><option>5</option>
        <label xml:lang="en">Depth of sections in main table of content</label>
        <label xml:lang="fr">Profondeur des sections dans le sommaire principal</label>
      </var>

      <var name="toc_with_abstract" type="boolean">
        <default>true</default>
        <label xml:lang="en">Abstracts in main table of content</label>
        <label xml:lang="fr">Résumés dans le sommaire principal</label>
      </var>

      <var name="subtoc" type="boolean">
        <default>true</default>
        <label xml:lang="en">With intermediate tables of content</label>
        <label xml:lang="fr">Avec sommaires intermédiaires</label>
      </var>

      <var name="index" type="boolean">
        <label xml:lang="en">With an index page</label>
        <label xml:lang="fr">Avec une page d'index</label>
      </var>

      <var name="nonav" type="boolean">
        <label xml:lang="en">Without navigation bar</label>
        <label xml:lang="fr">Sans barre de navigation</label>
      </var>
    </group>
  </variables>
</publiforge>
