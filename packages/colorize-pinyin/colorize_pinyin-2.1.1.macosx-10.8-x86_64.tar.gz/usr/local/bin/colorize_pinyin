#!/usr/local/opt/python3/bin/python3.4
# EASY-INSTALL-ENTRY-SCRIPT: 'colorize-pinyin==2.1.1','console_scripts','colorize_pinyin'
__requires__ = 'colorize-pinyin==2.1.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('colorize-pinyin==2.1.1', 'console_scripts', 'colorize_pinyin')()
    )
