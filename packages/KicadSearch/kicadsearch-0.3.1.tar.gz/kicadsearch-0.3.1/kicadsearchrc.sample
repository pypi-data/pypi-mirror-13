# configuration file for kicadsearch
# edit by your needs and copy to ~/.kicadsearchrc

[default]
# path to index directory
indexdir = ./test/data/index

[index]
# encoding in *.lib
encoding = latin1

# colon-separated list of library directories
librarydirs = ./test/data/library

# colon-separated list of module directories
moduledirs = ./test/data/library

[search]
