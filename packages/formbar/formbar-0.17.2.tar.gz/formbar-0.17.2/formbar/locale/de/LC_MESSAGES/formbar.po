#: formbar/config.py:426
msgid "This field is required. You must provide a value"
msgstr "Dieses Feld ist verpflichtend. Sie müssen einen Wert angeben."

#: formbar/config.py:431
msgid "This field is desired. Please provide a value"
msgstr "Dieses Feld ist erwünscht. Bitte geben Sie einen Wert an."

#: formbar/converters.py:65
#, python-format
msgid "Value '%s' must be in format 'HH:MM:SS'"
msgstr "Wert '%s' muss im Format 'HH:MM:SS' vorliegen"

#: formbar/converters.py:68
#, python-format
msgid "Value '%s' is too large"
msgstr "Wert '%s' ist zu groß"

#: formbar/converters.py:99
#, python-format
msgid "%s is not a valid date format."
msgstr "%s ist kein valides Format für ein Datum."

#: formbar/converters.py:143
#, python-format
msgid "%s is not a valid datetime format."
msgstr "%s ist kein valides Format für ein Datum/Zeit."

#: formbar/converters.py:193
#, python-format
msgid "%s is not a string value."
msgstr "%s is keine Zeichenkette."

#: formbar/converters.py:213
#, python-format
msgid "%s is not a integer value."
msgstr "%s ist kein Ganzzahlenwert."

#: formbar/converters.py:226
#, python-format
msgid "%s is not a float value."
msgstr "%s is kein Fließkommawert."

#: formbar/converters.py:236
#, python-format
msgid "%s is not valid email address."
msgstr "%s ist keine valide E-Mail-Adresse."

#: formbar/converters.py:247
#, python-format
msgid "%s is not a boolean value."
msgstr "%s ist kein boolescher Wert."

#: formbar/converters.py:257
#, python-format
msgid "%s is not a file value."
msgstr "%s ist keine Datei."

#: formbar/form.py:775
msgid "no selection"
