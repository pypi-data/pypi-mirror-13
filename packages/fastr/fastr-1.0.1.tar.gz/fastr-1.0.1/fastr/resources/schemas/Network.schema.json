{

    "type": "object",
    "class": "fastr.core.network.Network",
    "$schema": "http://json-schema.org/draft-04/schema",
    "required": ["id", "toolnodelist", "sourcelist", "constantlist", "sinklist", "linklist", "node_number", "link_number"],
    "definitions": {},
    "properties": {
        "id": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "node_number": {
            "type": "integer"
        },
        "link_number": {
            "type": "integer"
        },
        "sourcelist": {
            "type": "array",
            "uniqueItems": "true",
            "items": {"$ref": "SourceNode.schema.json#/"}
        },
        "constantlist": {
            "type": "array",
            "uniqueItems": "true",
            "items": {"$ref": "ConstantNode.schema.json#/"}
        },
        "sinklist": {
            "type": "array",
            "uniqueItems": "true",
            "items": {"$ref": "SinkNode.schema.json#/"}
        },
        "toolnodelist": {
            "type": "array",
            "uniqueItems": "true",
            "items": {"$ref": "Node.schema.json#/"}
        },
        "linklist": {
            "type": "array",
            "uniqueItems": "true",
            "items": {"$ref": "Link.schema.json#/"}
        },
        "preferred_types": {
            "type": "array",
            "uniqueItems": "true",
            "items": {"type": "string"}
        },
        "sourcegroups": {
            "type": "object",
            "patternProperties": {
                ".*": {
                    "type": "array",
                    "uniqueItems": "true",
                    "items": {"type": "string"}
                }
            }
        },
        "stepids": {
            "type": "object",
            "patternProperties": {
                ".*": {
                    "type": "array",
                    "uniqueItems": "true",
                    "items": {"type": "string"}
                }
            }
        }
    }
}